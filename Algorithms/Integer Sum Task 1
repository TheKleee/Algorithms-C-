public class Hello{
    public static void Main(){
        // Your code here!
                System.Console.WriteLine($"Sum of 3 and 4 is: " + Test(3, 4));
                System.Console.WriteLine($"Sum of 12 and 12 is not: " + Test(12, 12));
    }
    
    public static int Test(int x, int y){
        return x != y ? x + y :(x+ y) * 3;
    }
}
